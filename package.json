{
  "name": "loopstore-scraper",
  "version": "1.0.0",
  "description": "Scraper de produtos do AliExpress com filtros e exportação para Excel",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node main.js",
    "scrape": "node main.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=unitTests/jest.config.js",
