    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.1.2",
    "dotenv": "^16.4.5",
    "exceljs": "^4.3.0",
    "franc": "^6.2.0",
    "fs-extra": "^11.2.0",
    "imghash": "^1.1.0",
    "puppeteer": "^24.16.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
