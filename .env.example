# =================================
# EXEMPLO DE CONFIGURAÇÕES - ALIEXPRESS SCRAPER
# =================================
# Copie este arquivo para .env e configure suas variáveis

# API Keys
OPENAI_API_KEY=sk-your_openai_api_key_here

# Categorias de produtos (separadas por vírgula)
CATEGORIES=Casa & Cozinha,Tecnologia,Beleza,Lifestyle,Pets

# Filtros de produtos
MIN_SALES=500
MIN_REVIEWS=50
MIN_RATING=4.5
MIN_ORDERS=100
MAX_SHIPPING_COST=15.0
MIN_PROFIT_MARGIN=0.30

# Configurações de busca
MAX_PRODUCTS_RAW=80
TARGET_PRODUCTS_FINAL=20
MAX_PAGES_PER_CATEGORY=5

# Configurações do Chrome/Puppeteer
CHROME_PATH=C:/Program Files/Google/Chrome/Application/chrome.exe
CHROME_USER_DATA_DIR=
CHROME_PROFILE=
ENABLE_SCREENSHOTS=false

# Configurações gerais
DEFAULT_ZIPCODE=01001-000
DIRETORIO_DEBUG=scraper/debug_files

# Configurações de desenvolvimento
DEBUG=false
HEADLESS=true
SLOW_MO=0
PAGE_TIMEOUT=30000
ELEMENT_TIMEOUT=5000
NAVIGATION_TIMEOUT=30000
